<h1>Grupo de dados de <%= current_user.name %></h1>

<table class="table table-hover">
  <thead>
    <tr>
      <th>Titulo</th>
      <th>Ver</th>
      <th>Editar</th>
    </tr>
  </thead>
  <tbody>
  	<tr>
  		<% @initial_inputs.each do |initial_input| %>
	  		<td><%= initial_input.title %></td>
	  		<td><%= link_to raw('<i class="icon-info-sign icon-white"></i>'), initial_input, :class => 'btn btn-mini  btn-info' %></td>
	  		<td><%= link_to(raw('<i class="icon-pencil icon-white"></i>'), edit_initial_input_path(initial_input), :class => 'btn btn-mini btn-warning') %></td>
  		<% end -%>
  	</tr>
  </tbody>
 </table>